{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "@fastxe/gas-core#build": {
            "outputs": []
        },
        "@fastxe/gas-build#build": {
            "outputs": []
        },
        "typecheck": {
            "dependsOn": [
                "^build"
            ]
        },
        "push": {
            "dependsOn": [
                "build"
            ],
            "cache": false
        },
        "init": {
            "dependsOn": [
                "push"
            ],
            "cache": false
        },
        "deploy": {
            "dependsOn": [
                "init"
            ],
            "cache": false
        }
    }
}